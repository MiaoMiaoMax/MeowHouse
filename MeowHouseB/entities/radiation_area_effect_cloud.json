				"time_down_event": {
					"event": "meow:kill"
				}
			},
			"minecraft:conditional_bandwidth_optimization": {}
		},
	
		"events": {
			"meow:kill": {
				"add": {
					"component_groups": [
						"meow:kill"
					]
				}
			},
			"meow:explosion": {
				"add": {
					"component_groups": [
						"meow:explosion"
					]
				}
			}
		}
